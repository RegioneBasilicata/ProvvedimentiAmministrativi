                If Not rdr.IsDBNull(5) Then relatore.Carica = rdr.GetString(5)
                If Not rdr.IsDBNull(6) Then relatore.DataAttivazione = rdr.GetDateTime(6)
                If Not rdr.IsDBNull(7) Then relatore.DataDisttivazione = rdr.GetDateTime(7)
