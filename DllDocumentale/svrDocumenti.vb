                        Next
                    End If

                ElseIf oOperatore.Test_Attributo("LIVELLO_UFFICIO", "Responsabile") Then
                    vR = oUfficioDoc.UtentiUfficio("DETERMINE")
                    vR.Remove(oUfficioDoc.ResponsabileUfficio("DETERMINE"))
