                Log.Debug(Now & " -Parametri insufficienti, " + SFunzione)
                Throw New Exception("Parametri insufficienti, " + SFunzione)
            End If

            Dim sugg As ItemSuggerimentoInfo = Get_Suggerimento(suggerimento)

