        If Not DB Is Nothing Then
            Call DB.RollTrans()
            If DB.errore <> 0 Then
                Call SISTEMA.Registra_Log(DB.ErrDescr, SFunzione)
            End If
        End If
        GoTo FineSub

Herr:

        vRitPar(0) = Err.Number
        vRitPar(1) = Err.Description
