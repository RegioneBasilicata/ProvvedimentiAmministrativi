        End If



        Call DB.UpdateRS(RS)

        If DB.errore <> 0 Then
            vRitPar(0) = DB.errore
            vRitPar(1) = DB.ErrDescr
            GoTo RollTrans
        End If

        vR = DB_Aggiungi_RSLOG(RS, DBLOG, "Log_Documento")
        If vR(0) <> 0 Then
